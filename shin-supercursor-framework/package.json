{
  "name": "shin-supercursor-framework",
  "version": "1.0.0",
  "description": "Unified SuperCursor Framework - Framework-1 design + Framework-2 implementation with NestJS best practices",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "shin-supercursor": "dist/cli.js",
    "ssc": "dist/cli.js"
  },
  "scripts": {
    "build": "nest build",
    "build:watch": "nest build --watch",
    "dev": "nest start --watch",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:e2e": "jest --testPathPattern=test/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --config .eslintrc.cjs --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --config .eslintrc.cjs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "clean": "rimraf dist coverage .tsbuildinfo",
    "prebuild": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint:check && npm run test",
    "ci": "npm run validate && npm run build",
    "docs:generate": "typedoc --out docs src",
    "docs:serve": "http-server docs -p 8080",
    "prepare": "husky install || exit 0"
  },
  "keywords": [
    "nestjs",
    "cursor",
    "cli",
    "ai",
    "development",
    "automation",
    "personas",
    "code-generation",
    "typescript",
    "supercursor",
    "framework",
    "cqrs",
    "ddd",
    "clean-architecture"
  ],
  "author": "SuperCursor Framework Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yohi/shin-supercursor-framework.git"
  },
  "bugs": {
    "url": "https://github.com/yohi/shin-supercursor-framework/issues"
  },
  "homepage": "https://github.com/yohi/shin-supercursor-framework#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@types/helmet": "^0.0.48",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^11.1.0",
    "conf": "^11.0.2",
    "cosmiconfig": "^8.3.6",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.3.1",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "figures": "^6.0.1",
    "glob": "^10.3.10",
    "helmet": "^8.1.0",
    "highlight.js": "^11.9.0",
    "ini": "^4.1.1",
    "inquirer": "^9.2.12",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsondiffpatch": "^0.5.0",
    "keytar": "^7.9.0",
    "listr2": "^7.0.2",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.2",
    "minimatch": "^9.0.3",
    "nest-winston": "^1.9.4",
    "node-cache": "^5.1.2",
    "node-machine-id": "^1.1.12",
    "ora": "^7.0.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "postgres": "^3.3.5",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "sqlite3": "^5.1.6",
    "systeminformation": "^5.21.20",
    "toml": "^3.0.0",
    "turndown": "^7.1.2",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cross-spawn": "^6.0.4",
    "@types/express": "^4.17.17",
    "@types/ini": "^4.1.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/markdown-it": "^13.0.6",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.16",
    "@types/turndown": "^5.0.4",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "msw": "^2.0.8",
    "nock": "^13.4.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "cursor-cli": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "cursor-cli": {
      "optional": true
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapping": {
      "^@application/(.*)$": "<rootDir>/application/$1",
      "^@domain/(.*)$": "<rootDir>/domain/$1",
      "^@infrastructure/(.*)$": "<rootDir>/infrastructure/$1",
      "^@presentation/(.*)$": "<rootDir>/presentation/$1"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --config .eslintrc.cjs --fix",
      "prettier --write"
    ],
    "*.{json,md,yaml,yml}": [
      "prettier --write"
    ]
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/shin-supercursor-framework"
  }
}
