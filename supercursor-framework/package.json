{
  "name": "supercursor-framework",
  "version": "0.1.0",
  "description": "A comprehensive extension framework for Cursor CLI with specialized commands, AI personas, and enhanced development workflows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sc": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,json}",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build"
  },
  "keywords": [
    "cursor",
    "cli",
    "ai",
    "development",
    "framework",
    "automation",
    "personas",
    "code-generation"
  ],
  "author": "SuperCursor Framework Contributors",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.0.0",
    "commander": "^11.0.0",
    "inquirer": "^9.0.0",
    "chalk": "^5.0.0",
    "ora": "^7.0.0",
    "fs-extra": "^11.0.0",
    "glob": "^10.0.0",
    "yaml": "^2.3.0",
    "joi": "^17.9.0",
    "lodash": "^4.17.21",
    "axios": "^1.5.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.1.0"
  },
  "peerDependencies": {
    "cursor": ">=0.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperClaude-Org/SuperCursor_Framework.git"
  },
  "bugs": {
    "url": "https://github.com/SuperClaude-Org/SuperCursor_Framework/issues"
  },
  "homepage": "https://superclaude-org.github.io/"
}