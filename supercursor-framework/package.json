{
  "name": "supercursor-framework",
  "version": "0.1.0",
  "description": "SuperClaude-like framework that extends Cursor CLI with specialized commands, personas, and enhanced AI capabilities",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "supercursor": "dist/cli.js",
    "sc": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "jest --coverage tests",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "clean": "rimraf dist coverage .tsbuildinfo",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "prepare": "husky install",
    "docs:generate": "typedoc --out docs src",
    "docs:serve": "http-server docs -p 8080",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "ci": "npm run validate && npm run build"
  },
  "keywords": [
    "cursor",
    "cli",
    "ai",
    "development",
    "automation",
    "personas",
    "code-generation",
    "typescript",
    "superclaude"
  ],
  "author": "SuperCursor Framework Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yohi/supercursor-framework.git"
  },
  "bugs": {
    "url": "https://github.com/yohi/supercursor-framework/issues"
  },
  "homepage": "https://github.com/yohi/supercursor-framework#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "figures": "^6.0.1",
    "listr2": "^7.0.2",
    "conf": "^11.0.2",
    "cosmiconfig": "^8.3.6",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "axios": "^1.6.0",
    "ws": "^8.14.2",
    "chokidar": "^3.5.3",
    "glob": "^10.3.10",
    "minimatch": "^9.0.3",
    "fast-glob": "^3.3.2",
    "node-cache": "^5.1.2",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "semver": "^7.5.4",
    "yaml": "^2.3.4",
    "toml": "^3.0.0",
    "ini": "^4.1.1",
    "jsondiffpatch": "^0.5.0",
    "markdown-it": "^13.0.2",
    "highlight.js": "^11.9.0",
    "turndown": "^7.1.2",
    "keytar": "^7.9.0",
    "node-machine-id": "^1.1.12",
    "systeminformation": "^5.21.20",
    "execa": "^8.0.1",
    "cross-spawn": "^7.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/ws": "^8.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/uuid": "^9.0.6",
    "@types/ini": "^4.1.1",
    "@types/markdown-it": "^13.0.6",
    "@types/turndown": "^5.0.4",
    "@types/cross-spawn": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "prettier": "^3.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@jest/globals": "^29.7.0",
    "typescript": "^5.2.2",
    "tsx": "^4.1.2",
    "rimraf": "^5.0.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "typedoc": "^0.25.3",
    "http-server": "^14.1.1",
    "@types/supertest": "^2.0.16",
    "supertest": "^6.3.3",
    "msw": "^2.0.8",
    "nock": "^13.4.0"
  },
  "peerDependencies": {
    "cursor-cli": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "cursor-cli": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yaml,yml}": [
      "prettier --write"
    ]
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/supercursor-framework"
  }
}